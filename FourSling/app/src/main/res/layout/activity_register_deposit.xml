<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:fitsSystemWindows="true"
    android:background="@color/colorLine"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_title"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:layout_marginTop="@dimen/activity_height_ten"
                android:background="@drawable/activity_income_expend_item_background"
                android:paddingTop="@dimen/activity_height_twenty_five"
                android:paddingBottom="@dimen/activity_height_forty"
                android:layout_marginLeft="@dimen/activity_height_twenty_one"
                android:layout_marginRight="@dimen/activity_height_twenty_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/activity_register_deposit_item_title"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/colorBlackFour"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/activity_register_deposit_item_money"
                    android:layout_below="@+id/activity_register_deposit_item_title"
                    android:layout_marginTop="@dimen/activity_height_fifteen"
                    android:layout_centerHorizontal="true"
                    android:textColor="@color/colorRedTwo"
                    android:textSize="@dimen/activity_height_twenty_four"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="@dimen/activity_number_thirty"
                    android:id="@+id/activity_register_deposit_item_line"
                    android:layout_below="@+id/activity_register_deposit_item_money"
                    android:background="@drawable/activity_income_expend_item_textview_background"
                    android:layout_width="match_parent"
                    android:layout_marginLeft="@dimen/activity_height_twenty_five"
                    android:layout_marginRight="@dimen/activity_height_twenty_five"
                    android:layout_height="@dimen/activity_height_two" />
                <TextView
                    android:layout_marginTop="@dimen/activity_height_twenty_four"
                    android:id="@+id/activity_register_deposit_item_yve"
                    android:layout_below="@+id/activity_register_deposit_item_line"
                    android:layout_marginLeft="@dimen/activity_height_twenty_five"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:text="@string/fragment_conent_yv"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="@dimen/activity_height_twenty_four"
                    android:id="@+id/activity_register_deposit_item_yve_money"
                    android:layout_below="@+id/activity_register_deposit_item_line"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/activity_height_twenty_five"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:id="@+id/activity_register_deposit_item_fs_and_zh"
                    android:layout_below="@+id/activity_register_deposit_item_yve"
                    android:layout_marginLeft="@dimen/activity_height_twenty_five"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/activity_register_deposit_item_wx_and_alipay"
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:layout_marginRight="@dimen/activity_height_twenty_five"
                    android:layout_below="@+id/activity_register_deposit_item_yve_money"
                    android:layout_alignParentRight="true"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:id="@+id/activity_register_deposit_item_dh_and_sxf"
                    android:layout_below="@+id/activity_register_deposit_item_fs_and_zh"
                    android:layout_marginLeft="@dimen/activity_height_twenty_five"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/activity_register_deposit_item_odd_number"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/activity_height_twenty_five"
                    android:layout_below="@+id/activity_register_deposit_item_fs_and_zh"
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:visibility="gone"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:id="@+id/activity_register_deposit_item_xf_time"
                    android:layout_below="@+id/activity_register_deposit_item_dh_and_sxf"
                    android:text="时间"
                    android:layout_marginLeft="@dimen/activity_height_twenty_five"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
                <TextView
                    android:id="@+id/activity_register_deposit_item_time_nu"
                    android:layout_below="@+id/activity_register_deposit_item_odd_number"
                    android:layout_alignParentRight="true"
                    android:layout_marginRight="@dimen/activity_height_twenty_five"
                    android:layout_marginTop="@dimen/activity_height_eighteen"
                    android:textSize="@dimen/activity_height_fifteen"
                    android:textColor="@color/colorBlackFour"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />
            </RelativeLayout>
            <ImageView
                android:background="@mipmap/bg_detaillist"
                android:layout_marginLeft="@dimen/activity_height_twenty_one"
                android:layout_marginRight="@dimen/activity_height_twenty_one"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <include layout="@layout/loading"/>
    </FrameLayout>
</LinearLayout>